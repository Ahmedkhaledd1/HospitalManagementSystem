<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="operationScreenShower" prefHeight="500.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UIControllers.displayScheduleController">
   <children>
      <ComboBox fx:id="docMenuSched" layoutX="178.0" layoutY="48.0" onAction="#doctorSelectSchedFunc" prefWidth="150.0" promptText="Choose a Doctor" />
      <ComboBox fx:id="daysMenu" layoutX="178.0" layoutY="110.0" onAction="#dayScheduleSelectFunc" prefWidth="150.0" promptText="Choose a Day" />
      <Label layoutX="78.0" layoutY="52.0" text="Doctor" />
      <Label layoutX="78.0" layoutY="114.0" text="Day" />
      <Button fx:id="displaybtn" layoutX="152.0" layoutY="157.0" mnemonicParsing="false" onAction="#displayScheduleFunc" style="-fx-background-color: #0598ff;" text="Display" textFill="WHITE" />
      <ListView fx:id="TimeslotsList" layoutX="95.0" layoutY="229.0" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: #0598ff;" />
      <Label fx:id="displaySuccesslbl" layoutX="165.0" layoutY="449.0" text="Error or nottttttttttt" />
   </children>
</AnchorPane>
